@model TP2_ASP.NET_Final_.Models.Msg

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/PageDisposition.cshtml";
}




<div class="pcoded-inner-content">
    <div class="main-body">
        <div class="page-wrapper">
            <div class="page-body">
                <!-- [ page content ] start -->
                <div class="row">
                    <div class="col-lg-12 col-md-12">
                        <div class="card table-card review-card" style="width:320px">
                            <div style="margin-left:8px">
                                <h4>Message</h4>
                                <hr />
                                <dl class="dl-horizontal">
                                    <dt>
                                        Email
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.Email)
                                    </dd>

                                    <dt>
                                        User name
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.UserName)
                                    </dd>

                                    <dt>
                                        Message
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.mssg)
                                    </dd>

                                    <dt>
                                        Date message
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.DateM)
                                    </dd>

                                </dl>
                                <p>

                                    @Html.ActionLink("Back to List", "Index")
                                </p>
                            </div>
                           
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
